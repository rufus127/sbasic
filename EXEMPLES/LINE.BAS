   10 DIM LIGNE$(100)
   20 ON ERROR GOTO 1000
   30 OPEN OLD "FICHIER.TXT" AS 1
   35 I%=0
   40 INPUT LINE #1,LIGNE$(I%)
   50 I%=I%+1
   60 IF I%<=100 GOTO 40
   70 CLOSE 1
   80 FOR J%=0 TO I%-1
   90 PRINT LIGNE$(J%)
  100 NEXT J%
  110 END
 1000 IF ERR = 8 THEN PRINT I%;"lignes lues": RESUME 70
 1010 PRINT "Erreur";ERR;"… la ligne";ERL
