    IDS_PRINTERR, "Impossible d'imprimer le tampon impression!"
    IDS_PRINTCLEARERR, "Impossible de supprimer le tampon d'impression!"
    IDS_ZOOMF, "SBASIC (zoom %u%%)"
    IDS_WRITE_AS, "Ecriture de l'image dans"
    IDS_FILTER_BMP, "Fichier image (*.BMP)\0*.BMP\0Tous les fichiers\0*.*"
    IDS_PICTUREF, "Image%u"
    IDS_UCREATEIMAGE, "Impossible de créer le fichier image!"
    IDS_UWRITEIMAGE, "Impossible d'écrire le fichier image!"
    IDS_D3DDLL, "Dll Direct 3D DLL non disponible"
    IDS_D3DPROC, "Point d'entrée Direct 3D non trouvé"
    IDS_D3DOBJECT, "Erreur création object Direct 3D"
    IDS_D3DFORMAT, "Format surface Direct 3D non supportée"
