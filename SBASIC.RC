// SBASIC Resource description

#include "sb32.h"
#include "windows.h"

// Program Icon
SBASIC ICON DISCARDABLE "SBASIC.ICO"

// Print Dialog Box
PRINT DIALOG DISCARDABLE  32768, 0, 110, 22
STYLE WS_MINIMIZEBOX | WS_CAPTION | WS_SYSMENU
CAPTION "SBASIC"
CLASS "PRINT"
FONT 8, "MS Sans Serif"
BEGIN
END

// Menu
PRINT MENU DISCARDABLE 
BEGIN
    MENUITEM "&Editer tampon impression", IDM_PRINT
    MENUITEM "&Vider tampon impression", IDM_CLEARPRINT
END

// Help text
HELP TEXT LISEZMOI.TXT

// Messages
STRINGTABLE
BEGIN
    IDS_DELPRINT, "Supression fichier impression?"
